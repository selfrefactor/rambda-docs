{
  "name": "rambda-docs",
  "scripts": {
    "update": "ng update --all=true --allowDirty=true --next=true",
    "updatex": "ng update --all=true --allowDirty=true",
    "update:force": "ng update --force --all=true --allowDirty=true --next=true",
    "updatex:force": "ng update --force --all=true --allowDirty=true",
    "component": "ng g component --skipTests",
    "class": "ng g class --skipTests",
    "directive": "ng g directive --skipTests",
    "service": "ng g service --skipTests",
    "pipe": "ng g pipe --skipTests",
    "ng": "ng",
    "in": "ng serve",
    "start": "ng serve",
    "out": "ng build",
    "test": "ng test",
    "doc": "compodoc -p tsconfig.app.json"
  },
  "version": "1.0.0",
  "dependencies": {
    "@materia-ui/ngx-monaco-editor": "^4.0.1",
    "property-watch-decorator": "^1.2.5",
    "monaco-editor": "^0.20.0",
    "@angular/animations": "10.0.7",
    "@angular/cdk": "10.1.1",
    "@angular/common": "10.0.7",
    "@angular/compiler": "10.0.7",
    "@angular/core": "10.0.7",
    "@angular/forms": "10.0.7",
    "@angular/material": "10.1.1",
    "@angular/platform-browser": "10.0.7",
    "@angular/platform-browser-dynamic": "10.0.7",
    "@angular/router": "10.0.7",
    "@ctrl/ngx-codemirror": "4.0.0",
    "codemirror": "5.56.0",
    "grid-fn": "0.2.2",
    "rambdax": "4.0.0",
    "rxjs": "7.0.0-beta.0",
    "tslib": "^2.0.0",
    "zone.js": "0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.1000.5",
    "@angular/cli": "10.0.5",
    "@angular/compiler-cli": "10.0.7",
    "@angular/language-service": "10.0.7",
    "@babel/core": "7.11.0",
    "@compodoc/compodoc": "1.1.11",
    "@types/jest": "26.0.8",
    "@types/node": "14.0.27",
    "babel-loader": "8.1.0",
    "jest": "26.2.2",
    "jest-preset-angular": "8.2.1",
    "prettier": "2.0.5",
    "rxjs-tslint-rules": "4.34.0",
    "ts-node": "8.10.2",
    "tslint-fn": "0.2.2",
    "typescript": "3.9.7"
  },
  "jest": {
    "preset": "jest-preset-angular",
    "setupFilesAfterEnv": [
      "<rootDir>/setupJest.ts"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!@ngrx|angular2-ui-switch|ng-dynamic)"
    ]
  },
  "private": true,
  "commitMessage": {
    "workInProgress": ""
  }
}
