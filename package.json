{
  "name": "rambda-docs",
  "version": "1.0.0",
  "scripts": {
    "update": "ng update --all=true --allowDirty=true --next=true",
    "updatex": "ng update --all=true --allowDirty=true",
    "update:force": "ng update --force --all=true --allowDirty=true --next=true",
    "updatex:force": "ng update --force --all=true --allowDirty=true",
    "component": "ng g component --skipTests",
    "class": "ng g class --skipTests",
    "directive": "ng g directive --skipTests",
    "service": "ng g service --skipTests",
    "pipe": "ng g pipe --skipTests",
    "ng": "ng",
    "in": "ng serve",
    "start": "ng serve",
    "out": "ng build",
    "test": "ng test",
    "doc": "compodoc -p tsconfig.app.json"
  },
  "private": true,
  "jest": {
    "preset": "jest-preset-angular",
    "transformIgnorePatterns": [
      "node_modules/(?!@ngrx|angular2-ui-switch|ng-dynamic)"
    ]
  },
  "dependencies": {
    "@angular/animations": "^9.1.0",
    "@angular/cdk": "9.2.0",
    "@angular/common": "9.1.0",
    "@angular/compiler": "9.1.0",
    "@angular/core": "9.1.0",
    "@angular/forms": "9.1.0",
    "@angular/material": "9.2.0",
    "@angular/platform-browser": "9.1.0",
    "@angular/platform-browser-dynamic": "9.1.0",
    "@angular/router": "9.1.0",
    "@ctrl/ngx-codemirror": "^3.0.2",
    "codemirror": "^5.52.2",
    "grid-fn": "^0.0.4",
    "rambdax": "3.7.0",
    "rxjs": "^7.0.0-alpha.1",
    "tslib": "^1.11.1",
    "zone.js": "^0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.901.0",
    "@angular/cli": "9.1.0",
    "@angular/compiler-cli": "9.1.0",
    "@angular/language-service": "9.1.0",
    "@babel/core": "^7.9.0",
    "@compodoc/compodoc": "^1.1.11",
    "@types/jest": "^25.1.4",
    "@types/node": "^13.7.7",
    "babel-loader": "^8.1.0",
    "jest": "^25.2.3",
    "jest-preset-angular": "^8.1.2",
    "prettier": "2.0.2",
    "rxjs-tslint-rules": "^4.29.2",
    "ts-node": "^8.8.1",
    "tslint-fn": "0.2.1",
    "typescript": "3.8.3"
  },
  "commitMessage": {
    "workInProgress": ""
  }
